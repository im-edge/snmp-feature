{
  "name": "imedge/snmp-feature",
  "type": "project",
  "config": {
    "platform": {
      "php": "8.1.18"
    },
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "IMEdge\\SnmpFeature\\": "src"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "imedge/node-provided-libs": "^0.12",
    "imedge/snmp": ">=0.4",
    "imedge/snmp-client": "^0.2"
  },
  "require-dev": {
    "ramsey/uuid": "^4",
    "phpstan/phpstan": "^1.10 | ^2",
    "phpunit/phpunit": "^10.3",
    "squizlabs/php_codesniffer": "^3.7 | ^4.0",
    "symplify/easy-coding-standard": "^12.0"
  }
}
